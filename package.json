  },
  "homepage": "https://github.com/oddnetworks/oddworks",
  "dependencies": {
    "bcrypt": "~1.0.0",
    "bluebird": "~3.5.0",
    "body-parser": "~1.17.0",
    "boom": "~4.3.0",
    "debug": "~2.6.0",
    "dynamodb-marshaler": "~2.0.0",
    "express": "~4.15.0",
    "header-parser": "~1.0.0",
    "jsonwebtoken": "~7.3.0",
    "lodash": "~4.17.0",
    "node-uuid": "~1.4.0",
    "object-property-natural-sort": "0.0.4",
    "oddcast": "~2.2.0",
    "redis-search": "~0.0.1",
    "reds": "~0.2.0",
