    "boom": "7.x.x",
    "debug": "3.x.x",
    "express": "4.x.x",
    "header-parser": "1.x.x",
    "jsonwebtoken": "^8.2.2",
    "lodash": "4.x.x",
    "object-property-natural-sort": "0.0.4",
